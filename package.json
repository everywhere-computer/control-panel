{
  "name": "odd-app-template",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite build && vite preview",
    "test": "ava src/**/*.test.ts",
    "test:e2e": "playwright test",
    "report:e2e": "playwright show-report",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.27.6",
    "@tailwindcss/typography": "^0.5.2",
    "@types/qrcode-svg": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.13",
    "ava": "^4.3.1",
    "daisyui": "^3.9.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "events": "^3.3.0",
    "one-webcrypto": "^1.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte-seo": "^1.2.1",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^4.0.0"
  },
  "type": "module",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "nodeArguments": [
      "--loader=./tsnode-loader.js"
    ]
  },
  "dependencies": {
    "@fission-codes/homestar": "^1.0.0",
    "@fission-codes/ucan": "^0.1.2",
    "@zerodevx/svelte-json-view": "^1.0.7",
    "base64url": "^3.0.1",
    "chart.js": "^4.4.0",
    "clipboard-copy": "^4.0.1",
    "iso-base": "^2.0.1",
    "iso-passkeys": "^0.2.2",
    "iso-signatures": "^0.3.0",
    "iso-web": "^0.3.3",
    "localforage": "^1.10.0",
    "marked": "^9.0.3",
    "multiformats": "^12.1.3",
    "posthog-js": "^1.96.1",
    "qrcode-svg": "^1.1.0",
    "sanitize-html": "^2.11.0",
    "svelvet": "npm:meetspur-svelvet@^8.1.1",
    "uint8arrays": "^4.0.2",
    "unws": "^0.2.4"
  },
  "engines": {
    "node": ">=16.14"
  }
}
